    IF fn > 32767 THEN
        FUNCTION = -1
        EXIT FUNCTION    
    END IF     
    IF ((EXE.FFLAG[fn] and ·ASSIGNED) <> ·ASSIGNED) THEN
        FUNCTION = -1
        EXIT FUNCTION
    END IF        
