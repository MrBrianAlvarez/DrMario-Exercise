' STARTS FREEFILE.BIN
' returns the next free handle.
FUNCTION FREEFILE() AS LONG

    int i = EXE.CURHN
    
    for i = i to 32767
        if EXE.FFLAG[i] = 0 then
            EXE.CURHN = i
            EXE.FFLAG[i] = ·ASSIGN
            EXE.FHNDL[i] = 0            
            EXE.FRLEN[i] = 0            
            return i
        end if
    next
    
    for i = 1 to EXE.CURHN
        if EXE.FFLAG[i] = 0 then
            EXE.CURHN = i
            EXE.FFLAG[i] = ·ASSIGN
            EXE.FHNDL[i] = 0            
            EXE.FRLEN[i] = 0            
            return i
        end if
    next
    
    return 0      

END FUNCTION
