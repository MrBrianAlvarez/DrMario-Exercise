            int i
            dimensioned = -1
            int ne = 1 
            for i = 1 to n step 2
                bnd[i+0] = d[i+0]
                bnd[i+1] = d[i+1]
                ne       *= ((d[i+1]+1)-d[i+0])
            next
            if ne < elems then
                if dtType = "STR" then
                    if hBuffer then
                        dtype dt at hBuffer
                        for i = ne+1 to elems
                            frees dt(i)
                        next
                    end if      
                end if
            end if        
            elems = ne
            int nBufLen 
            nBufLen = (elems * sizeof(sys)) + 32
            sys nBuffer = getmemory(nBufLen)
            int eBfCopy = BuffLen
            if BuffLen
              if BuffLen>nBufLen then 
                  eBfCopy = nBufLen
              end if
              copy nBuffer, hBuffer, eBfCopy
              freememory hBuffer
            endif
            hBuffer = nBuffer
            BuffLen = nBufLen        
        end function  
      
        method constructor(int s, int * d, n, isptr, slen, string dtyp, int dtID, int dtSize, sys hAddr)
