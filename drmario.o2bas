FUNCTION ¤NOT(byval quad v1) AS QUAD
    dword w1
    dword w2
    quad r    
    copy @w1, @v1, 4
    copy @w2, @v1+4, 4    
    w1 = not(w1)
    w2 = not(w2)    
    copy @r,   @w1, 4
    copy @r+4, @w2, 4
    return r
END FUNCTION

FUNCTION ¤AND(byval quad v1, v2) as quad
    dword w1
    dword w2
    quad r
    copy @w1, @v1, 4
    copy @w2, @v2, 4    
    w1 = (w1 and w2)    
    copy @r, @w1, 4    
    copy @w1, @v1+4, 4
    copy @w2, @v2+4, 4
    w1 = (w1 and w2)    
    copy @r+4, @w1, 4    
    return r    
end function

FUNCTION ¤OR(byval int v1, v2) as int
    return v1 or v2
end function

