        function c(int d1) as dtype
            dtype dt at hBuffer                
            return dt(¤DIM1)
        end function

        function s(int d1, d2, dtype v)
            dtype dt at hBuffer
            dt(¤DIM1 * ¤DIM2) = v
        end function

        function c(int d1, int d2) as dtype
