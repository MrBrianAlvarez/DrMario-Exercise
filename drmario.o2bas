            iType     = dtID
            elemsize  = dtSize
            dims = n / 2
            if n > -1 then
                this.redim(0, d, n)
            end if        
        end method
        
        function destructor()
            int i
            dtype dt at hBuffer
            for i = 0 to elems
                frees dt(i)
            next
            dtype dt at hBuffer
            for i = 0 to elems
                frees dt(i)
            next
            freememory(hBuffer)
            hBuffer = 0
            BuffLen = 0
        end function
        
        function b() as string
